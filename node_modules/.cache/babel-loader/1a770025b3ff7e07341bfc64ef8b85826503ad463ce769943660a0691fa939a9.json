{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapActions } from \"vuex\";\nimport { VueEditor } from \"vue2-editor\";\nexport default {\n  name: \"AddPho\",\n  components: {\n    VueEditor\n  },\n  data() {\n    return {\n      status: \"\",\n      message: \"\",\n      phone: \"\",\n      type: \"OFICINA\",\n      link: \"\",\n      //MODAL\n      files: [],\n      viewModal: false,\n      search_value: \"\"\n    };\n  },\n  computed: {\n    identity() {\n      return this.$store.getters[\"admin/getIdentity\"];\n    }\n  },\n  methods: {\n    ...mapActions(\"phone\", [\"setAddedPho\"]),\n    ...mapActions(\"phone\", [\"setHistoryOptionPho\"]),\n    ...mapActions(\"colaborador\", [\"getAllInfoClb\"]),\n    ...mapActions(\"phone\", [\"getInfoByIdPho\"]),\n    previewFiles() {\n      this.files = this.$refs.myFiles.files;\n    },\n    addEtiqueta(value) {\n      if (value) {\n        let newarr = [];\n        for (const item of this.etiquetas) {\n          newarr.push(item);\n        }\n        newarr.push(value);\n        this.etiquetas = newarr;\n        this.etiqueta = \"\";\n      }\n    },\n    delEtiqueta(index) {\n      let newarr = [];\n      for (var i = 0; i < this.etiquetas.length; i++) {\n        if (i != index) newarr.push(this.etiquetas[i]);\n      }\n      this.etiquetas = newarr;\n    },\n    wait: function () {\n      setTimeout(() => this.$router.go(), 200);\n    },\n    includesItem(search, name) {\n      name = name.normalize(\"NFD\").replace(/[\\u0300-\\u036f]/g, \"\");\n      if (search == \"\") {\n        return true;\n      }\n      name = name.toLowerCase();\n      search = search.toLowerCase();\n      if (search == \"\") {\n        return false;\n      }\n      return name.includes(search);\n    },\n    submit: async function (phone, type, link) {\n      this.status = \"\";\n      this.message = \"\";\n      let linkC;\n      if (type == 'WHATSAPP') {\n        linkC = link;\n      } else if (type == 'OFICINA') {\n        linkC = 'tel:+' + phone;\n      } else if (type == 'CELULAR') {\n        linkC = 'tel:+52' + phone;\n      }\n      let result = await this.$store.dispatch(\"phone/addItemPho\", {\n        option: \"phone\",\n        item: {\n          phone,\n          type,\n          link: linkC\n        }\n      });\n      if (result.status == \"error\") {\n        this.status = \"error\";\n        this.message = result.message;\n      } else {\n        // success\n        this.showModal();\n      }\n    },\n    delStatus: function () {\n      setTimeout(() => this.delMsgs(), 2000);\n    },\n    delMsgs: function () {\n      this.status = \"\";\n      this.message = \"\";\n    },\n    showModal: function () {\n      this.viewModal = true;\n    },\n    Add: function () {\n      this.status = \"\";\n      this.message = \"\";\n      this.phone = \"\";\n      this.type = \"\";\n      this.link = \"\";\n      this.viewModal = false;\n      this.setAddedPho(\"\");\n    },\n    toEdit: function () {\n      this.setHistoryOptionPho(\"Edit\");\n      this.viewModal = false;\n    },\n    back: function () {\n      setTimeout(() => this.$router.go(), 200);\n    }\n  }\n};","map":{"version":3,"mappings":";AA0HA;AACA;AACA;EACAA;EACAC;IACAC;EACA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACA;MACAC;MACAC;MAEAC;IACA;EACA;EACAC;IACAC;MACA;IACA;EACA;EACAC;IACA;IACA;IACA;IACA;IACAC;MACA;IACA;IACAC;MACA;QACA;QACA;UACAC;QACA;QACAA;QACA;QACA;MACA;IACA;IACAC;MACA;MACA;QACA;MACA;MACA;IACA;IACAC;MACAC;IACA;IACAC;MACArB;MACA;QACA;MACA;MACAA;MACAsB;MACA;QACA;MACA;MACA;IACA;IACAC;MACA;MACA;MACA;MAEA;QACAC;MACA,OACA,uBACA;QACAA;MACA,OACA,uBACA;QACAA;MACA;MAGA;QACAC;QACAC;UACApB;UACAC;UACAC;QACA;MACA;MAEA;QACA;QACA;MACA;QACA;QACA;MACA;IACA;IACAmB;MACAP;IACA;IACAQ;MACA;MACA;IACA;IAEAC;MACA;IACA;IAEAC;MACA;MACA;MACA;MACA;MACA;MAEA;MACA;IACA;IACAC;MACA;MACA;IACA;IACAC;MACAZ;IACA;EACA;AACA","names":["name","components","VueEditor","data","status","message","phone","type","link","files","viewModal","searc