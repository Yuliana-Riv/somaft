>\r\n            <input\r\n              v-model=\"link\"\r\n              type=\"text\"\r\n              id=\"titulo\"\r\n              name=\"titulo\"\r\n              class=\"wd-lrg\"\r\n              required\r\n            />\r\n          </div>\r\n        </div>\r\n\r\n        <!-- <div class=\"grup-form dflx\">\r\n        \r\n        <div class=\"boxform\">\r\n          <label for=\"descripcion\" class=\"descripcion\">Descripción</label>\r\n          <textarea\r\n            v-model=\"descripcion\"\r\n            class=\"format_ta\"\r\n            id=\"descripcion\"\r\n            name=\"descripcion\"\r\n          />\r\n        </div>\r\n     \r\n        \r\n      </div> -->\r\n\r\n        <!-- <div class=\"grup-form dflx\">\r\n          <div class=\"boxform\">\r\n            <label for=\"iframe\" class=\"iframe\">Mapa iframe (opcional) </label>\r\n            <input\r\n              v-model=\"iframe\"\r\n              type=\"text\"\r\n              id=\"iframe\"\r\n              name=\"iframe\"\r\n              class=\"wd-lrg\"\r\n            />\r\n          </div>\r\n         \r\n          \r\n        </div> -->\r\n\r\n        <div v-if=\"status != ''\" class=\"status_messages\">\r\n          <div v-if=\"status == 'success'\" class=\"msg msg_success\">\r\n            <p>{{ message }}</p>\r\n          </div>\r\n          <div v-if=\"status == 'error'\" class=\"msg msg_error\">\r\n            <p>{{ message }}</p>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"dflx\">\r\n          <p @click=\"wait()\" class=\"btnRegresar\">Regresar</p>\r\n          <button class=\"alta\" type=\"submit\">Guardar</button>\r\n        </div>\r\n      </form>\r\n\r\n      <div v-if=\"viewModal\" class=\"modal_view\">\r\n        <div id=\"modal_add\">\r\n          <div class=\"body dflx\">\r\n            <h3>Teléfono agregado</h3>\r\n            <img src=\"../../assets/default/add.png\" alt=\"icono alerta\" />\r\n          </div>\r\n\r\n          <div class=\"modal_add_btns dflx\">\r\n            <p @click=\"Add()\" class=\"otro\">Aceptar</p>\r\n            <p @click=\"toEdit()\" class=\"editar\">Editar</p>\r\n            <p @click=\"back()\" class=\"inicio\">Inicio</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { mapActions } from \"vuex\";\r\nimport { VueEditor } from \"vue2-editor\";\r\nexport default {\r\n  name: \"AddPho\",\r\n  components: {\r\n    VueEditor,\r\n  },\r\n  data() {\r\n    return {\r\n      status: \"\",\r\n      message: \"\",\r\n      phone: \"\",\r\n      type: \"OFICINA\",\r\n      link: \"\",\r\n      //MODAL\r\n      files: [],\r\n      viewModal: false,\r\n\r\n      search_value: \"\",\r\n    };\r\n  },\r\n  computed: {\r\n    identity() {\r\n      return this.$store.getters[\"admin/getIdentity\"];\r\n    },\r\n  },\r\n  methods: {\r\n    ...mapActions(\"phone\", [\"setAddedPho\"]),\r\n    ...mapActions(\"phone\", [\"setHistoryOptionPho\"]),\r\n    ...mapActions(\"colaborador\", [\"getAllInfoClb\"]),\r\n    ...mapActions(\"phone\", [\"getInfoByIdPho\"]),\r\n    previewFiles() {\r\n      this.files = this.$refs.myFiles.files;\r\n    },\r\n    addEtiqueta(value) {\r\n      if (value) {\r\n        let newarr = [];\r\n        for (const item of this.etiquetas) {\r\n          newarr.push(item);\r\n        }\r\n        newarr.push(value);\r\n        this.etiquetas = newarr;\r\n        this.etiqueta = \"\";\r\n      }\r\n    },\r\n    delEtiqueta(index) {\r\n      let newarr = [];\r\n      for (var i = 0; i < this.etiquetas.length; i++) {\r\n        if (i != index) newarr.push(this.etiquetas[i]);\r\n      }\r\n      this.etiquetas = newarr;\r\n    },\r\n    wait: function () {\r\n      setTimeout(() => this.$router.go(), 200);\r\n    },\r\n    includesItem(search, name) {\r\n      name = name.normalize(\"NFD\").replace(/[\\u0300-\\u036f]/g, \"\");\r\n      if (search == \"\") {\r\n        return true;\r\n      }\r\n      name = name.toLowerCase();\r\n      search = search.toLowerCase();\r\n      if (search == \"\") {\r\n        return false;\r\n      }\r\n      return name.includes(search);\r\n    },\r\n    submit: async function (phone, type, link) {\r\n      this.status = \"\";\r\n      this.message = \"\";\r\n      let linkC;\r\n\r\n      if (type == 'WHATSAPP') {\r\n        linkC = link;\r\n      }\r\n      else if (type == 'OFICINA')\r\n      {\r\n        linkC = 'tel:+'+phone;\r\n      }\r\n      else if (type == 'CELULAR')\r\n      {\r\n        linkC = 'tel:+52'+phone;\r\n      }\r\n      \r\n\r\n      let result = await this.$store.dispatch(\"phone/addItemPho\", {\r\n        option: \"phone\",\r\n        item: {\r\n          phone,\r\n          type,\r\n          link: linkC, \r\n        },\r\n      });\r\n\r\n      if (result.status == \"error\") {\r\n        this.status = \"error\";\r\n        this.message = result.message;\r\n      } else {\r\n        // success\r\n        this.showModal();\r\n      }\r\n    },\r\n    delStatus: function () {\r\n      setTimeout(() => this.delMsgs(), 2000);\r\n    },\r\n    delMsgs: function () {\r\n      this.status = \"\";\r\n      this.message = \"\";\r\n    },\r\n\r\n    showModal: function () {\r\n      this.viewModal = true;\r\n    },\r\n\r\n    Add: function () {\r\n      this.status = \"\";\r\n      this.message = \"\"; \r\n      this.phone = \"\";\r\n      this.type = \"\";\r\n      this.link = \"\"; \r\n\r\n      this.viewModal = false;\r\n      this.setAddedPho(\"\");\r\n    },\r\n    toEdit: function () {\r\n      this.setHistoryOptionPho(\"Edit\");\r\n      this.viewModal = false;\r\n    },\r\n    back: function () {\r\n      setTimeout(() => this.$router.go(), 200);\r\n    }, \r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n#AddPho label {\r\n  color: var(--color-2);\r\n  font-weight: 700;\r\n  font-size: 0.7291666666666666vw;\r\n  margin-bottom: 0.3vw;\r\n}\r\n\r\n#AddPho input,\r\n#AddPho select {\r\n  height: 2.5vw;\r\n  width: 14.