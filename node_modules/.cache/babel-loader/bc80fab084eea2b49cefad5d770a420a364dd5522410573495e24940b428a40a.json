{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapActions } from \"vuex\";\nimport { url } from '../../global/index';\nimport { VueEditor } from \"vue2-editor\";\nexport default {\n  name: \"EditLcl\",\n  components: {\n    VueEditor\n  },\n  data() {\n    return {\n      status: \"\",\n      message: \"\",\n      status2: \"\",\n      message2: \"\",\n      active: false,\n      url: url,\n      archivo: \"No se eligió archivo\",\n      statusimg: false,\n      //MODAL\n      file1: null,\n      viewModal: false,\n      search_value: '',\n      etiqueta: '',\n      files: []\n    };\n  },\n  async created() {\n    await this.getAllInfoClb('colaborador');\n  },\n  computed: {\n    added() {\n      return this.$store.getters[\"locales/getAdded\"];\n    },\n    identity() {\n      return this.$store.getters[\"admin/getIdentity\"];\n    },\n    colaboradores() {\n      let data = this.$store.getters[\"colaborador/data\"];\n      if (!Array.isArray(data)) return [];\n      if (this.identity.role == 'colaborador') data = data.filter(item => item.id == this.identity.sub);\n      if (this.search_value != '') {\n        let filt = data.filter(item => {\n          const fullname = item.name + ' ' + item.lastname;\n          return this.includesItem(this.search_value, fullname);\n        });\n        data = filt;\n      }\n      return data;\n    },\n    data() {\n      return this.$store.getters[\"locales/data\"];\n    }\n  },\n  methods: {\n    ...mapActions(\"locales\", [\"setAddedLcl\"]),\n    ...mapActions(\"locales\", [\"getAllInfoClb\"]),\n    ...mapActions(\"locales\", [\"getInfoByIdLcl\"]),\n    ...mapActions(\"colaborador\", [\"getAllInfoClb\"]),\n    updateAdded: async function () {\n      if (!this.added.id) return setTimeout(() => this.$router.go(), 200);\n      const result = await this.getInfoByIdLcl({\n        id: this.added.id,\n        option: 'locales'\n      });\n      if (result.id) {\n        this.setAddedLcl(result);\n      }\n    },\n    addEtiqueta(value) {\n      if (value) {\n        let newarr = [];\n        for (const item of this.added.etiquetas) {\n          newarr.push(item);\n        }\n        newarr.push(value);\n        this.added.etiquetas = newarr;\n        this.etiqueta = '';\n      }\n    },\n    delEtiqueta(index) {\n      let newarr = [];\n      for (var i = 0; i < this.added.etiquetas.length; i++) {\n        if (i != index) newarr.push(this.added.etiquetas[i]);\n      }\n      this.added.etiquetas = newarr;\n    },\n    includesItem(search, name) {\n      name = name.normalize(\"NFD\").replace(/[\\u0300-\\u036f]/g, \"\");\n      if (search == '') {\n        return true;\n      }\n      name = name.toLowerCase();\n      search = search.toLowerCase();\n      if (search == '') {\n        return false;\n      }\n      return name.includes(search);\n    },\n    wait: function () {\n      setTimeout(() => this.$router.go(), 200);\n    },\n    ...mapActions(\"locales\", [\"getInfoByIdLcl\"]),\n    submit: async function (id_colaborador, titulo, estatus, renta, tipo, descripcion, ubicacion, precio, superficie, iframe, detalles, etiquetas) {\n      this.status = \"\";\n      this.message = \"\";\n      let result = await this.$store.dispatch(\"locales/editItemLcl\", {\n        option: \"locales\",\n        item: {\n          id: this.added.id,\n          id_colaborador,\n          titulo,\n          estatus,\n          renta,\n          tipo,\n          descripcion,\n          ubicacion,\n          precio,\n          superficie,\n          iframe,\n          detalles,\n          etiquetas\n        }\n      });\n      if (result.status == \"error\") {\n        this.status = \"error\";\n        this.message = result.message;\n      } else {\n        // success\n        this.showModal();\n      }\n    },\n    updatePortada: async function (id, id_local) {\n      let result = await this.$store.dispatch(\"locales/editItemLcl\", {\n        option: \"locales/portada\",\n        item: {\n          id,\n          id_local\n        }\n      });\n      if (result.status == \"error\") {\n        this.status = \"error\";\n        this.message = result.message;\n      } else {\n        // success\n        this.updateAdded();\n      }\n    },\n    deleteFoto: async function (id) {\n      this.status = '';\n      this.message = '';\n      let result = await this.$store.dispatch(\"locales/deleteItemLcl\", {\n        option: 'locales/foto',\n        id: id\n      });\n      if (result.status == 'error') {\n        this.message = result.message;\n        this.status = 'error';\n        this.delStatus();\n      } else {\n        this.status = '';\n        this.message = '';\n        this.updateAdded();\n      }\n    },\n    submitFiles: async function (files) {\n      this.status = \"\";\n      this.message = \"\";\n      if (files.length > 0) {\n        const id = this.added.id;\n        for (const image of files) {\n          //Creamos el formData\n          var data = new FormData();\n          //Añadimos la imagen seleccionada\n          data.append(\"image\", image);\n          data.append(\"id\", id);\n\n          //Añadimos el método PUT dentro del formData\n          // Como lo hacíamos desde un formulario simple _(no ajax)_\n          data.append(\"_method\", \"POST\");\n          await this.$store.dispatch(\"locales/addItemLcl\", {\n            option: \"locales/foto\",\n            item: data\n          });\n        }\n\n        // success\n        this.files = [];\n        this.updateAdded();\n      } else {\n        this.status = \"error\";\n        this.message = 'No has adjuntado ninguna foto.';\n        this.delStatus();\n      }\n    },\n    previewFiles() {\n      this.files = this.$refs.myFiles.files;\n    },\n    delStatus: function () {\n      setTimeout(() => this.delMsgs(), 2000);\n    },\n    delMsgs: function () {\n      this.status = \"\";\n      this.message = \"\";\n    },\n    showModal: function () {\n      this.viewModal = true;\n    },\n    toolbar: function () {\n      return [['bold', 'italic', 'underline', 'strike'],\n      // toggled buttons\n      ['blockquote', 'code-block'], [{\n        'header': 1\n      }, {\n        'header': 2\n      }],\n      // custom button values\n      [{\n        'list': 'ordered'\n      }, {\n        'list': 'bullet'\n      }], [{\n        'script': 'sub'\n      }, {\n        'script': 'super'\n      }],\n      // superscript/subscript\n      [{\n        'indent': '-1'\n      }, {\n        'indent': '+1'\n      }],\n      // outdent/indent\n      [{\n        'direction': 'rtl'\n      }],\n      // text direction\n\n      [{\n        'size': ['small', false, 'large', 'huge']\n      }],\n      // custom dropdown\n      [{\n        'header': [1, 2, 3, 4, 5, 6, false]\n      }],\n      // [ 'link','image' ,'video' ],          // add's image support\n      [{\n        'color': []\n      }, {\n        'background': []\n      }],\n      // dropdown with defaults from theme\n      [{\n        'font': []\n      }], [{\n        'align': []\n      }], ['clean'] // remove formatting button\n      ];\n    }\n  }\n};","map":{"version":3,"mappings":";AAkVA;AACA;AACA;AAEA;EACAA;EACAC;IACAC;EACA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACA;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EAEA;IAGA;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACA;IACA;IACAC;MACA;MAEA;MACA;MACA;QAEA;UACA;UACA;QACA;QACAjB;MACA;MAEA;IACA;IACAA;MACA;IACA;EACA;EACAkB;IACA;IACA;IACA;IACA;IAGAC;MACA;MAEA;QACAC;QACAC;MACA;MAEA;QACA;MACA;IACA;IACAC;MACA;QACA;QACA;UACAC;QACA;QACAA;QACA;QACA;MACA;IACA;IACAC;MAGA;MACA;QACA;MACA;MACA;IAGA;IAEAC;MACA5B;MACA;QACA;MACA;MACAA;MACA6B;MACA;QACA;MACA;MACA;IACA;IAGAC;MACAC;IACA;IACA;IACAC;MACA;MACA;MAIA;QACAR;QACAS;UAAAV;UAAAW;UAAAC;UAAAC;UAAAC;UAAAC;UAAAC;UAAAC;UAAAC;UAAAC;UAAAC;UAAAC;UAAAC;QAAA;MACA;MAEA;QACA;QACA;MACA;QACA;QACA;MACA;IAEA;IACAC;MACA;QACAtB;QACAS;UAAAV;UAAAwB;QAAA;MACA;MAEA;QACA;QACA;MACA;QACA;QACA;MACA;IACA;IACAC;MAEA;MACA;MAEA;QAAAxB;QAAAD;MAAA;MAEA;QACA;QACA;QACA;MAEA;QACA;QACA;QACA;MACA;IACA;IACA0B;MACA;MACA;MAGA;QAEA;QAEA;UACA;UACA;UACA;UACA9C;UACAA;;UAEA;UACA;UACAA;UAEA;YACAqB;YACAS;UACA;QACA;;QAIA;QACA;QACA;MAIA;QACA;QACA;QACA;MACA;IAKA;IAEAiB;MACA;IACA;IACAC;MACApB;IACA;IACAqB;MACA;MACA;IACA;IAEAC;MACA;IACA;IAGAC;MACA,QACA;MAAA;MACA,8BAEA;QAAA;MAAA;QAAA;MAAA;MAAA;MACA;QAAA;MAAA;QAAA;MAAA,IACA;QAAA;MAAA;QAAA;MAAA;MAAA;MACA;QAAA;MAAA;QAAA;MAAA;MAAA;MACA;QAAA;MAAA;MAAA;;MAEA;QAAA;MAAA;MAAA;MACA;QAAA;MAAA;MACA;MACA;QAAA;MAAA;QAAA;MAAA;MAAA;MACA;QAAA;MAAA,IACA;QAAA;MAAA,IAEA;MAAA,CACA;IACA;EAIA;AACA","names":["name","components","VueEditor","data","status","message","status2","message2","active","url","archivo","statusimg","file1","viewModal","search_value","etiqueta","files","computed","added","identity","colaboradores","methods","updateAdded","id","option","addEtiqueta","newarr","delEtiqueta","includesItem","search","wait","setTimeout","submit","item","id_colaborador","titulo","estatus","renta","tipo","descripcion","ubicacion","precio","superficie","iframe","detalles","etiquetas","updatePortada","id_local","deleteFoto","submitFiles","previewFiles","delStatus","delMsgs","showModal","toolbar"],"sourceRoot":"src/views/locales","sources":["editLcl.vue"],"sourcesContent":["<template >\r\n  <div id=\"EditLcl\">\r\n    <div class=\"form-content\">\r\n\r\n      <form\r\n        @submit.prevent=\"\r\n          submit(\r\n            added.id_colaborador , added.titulo , added.estatus, added.renta, added.tipo, \r\n            added.descripcion, added.ubicacion, added.precio, added.superficie, added.iframe, added.detalles, added.etiquetas\r\n            \r\n          )\r\n        \"\r\n      >\r\n\r\n      <div class=\"grup-form dflx\">\r\n          <div class=\"boxform\">\r\n            <label for=\"titulo\" class=\"titulo\">Título </label>\r\n            <input\r\n              v-model=\"added.titulo\"\r\n              type=\"text\"\r\n              id=\"titulo\"\r\n              name=\"titulo\"\r\n              class=\"wd-lrg\"\r\n              required\r\n            />\r\n          </div>\r\n         \r\n          \r\n        </div>\r\n\r\n      <!-- <div class=\"grup-form dflx\">\r\n        \r\n        <div class=\"boxform\">\r\n          <label for=\"descripcion\" class=\"descripcion\">Descripción</label>\r\n          <textarea\r\n            v-model=\"added.descripcion\"\r\n            class=\"format_ta\"\r\n            id=\"descripcion\"\r\n            name=\"descripcion\"\r\n          />\r\n        </div>\r\n     \r\n        \r\n      </div> -->\r\n      <div class=\"grup-form dflx\">\r\n        \r\n    \r\n        <div class=\"boxform\">\r\n          <label for=\"colaborador\" class=\"colaborador\">Colaborador</label>\r\n          <input\r\n              v-model=\"added.colb_fullname\"\r\n              type=\"text\"\r\n              id=\"colaborador\"\r\n              name=\"colaborador\"\r\n              required\r\n              disabled\r\n            />\r\n\r\n            <div>\r\n            \r\n              <div class=\"bdy_tbl\">\r\n                 <div v-for=\"(item, index) in colaboradores\" :key=\"index\">\r\n                  <p @click=\"added.id_colaborador = item.id, added.colb_fullname = (item.name +' '+item.lastname)\" :class=\"{'active_item' : added.id_colaborador == item.id}\" >{{ item.name }} {{ item.lastname }} </p>\r\n                 </div>\r\n              </div>\r\n            </div>\r\n\r\n            <input\r\n              v-model=\"search_value\"\r\n              type=\"text\"\r\n              :placeholder=\"'Buscar'\"\r\n              class=\"inp_buscar\"\r\n            />\r\n        </div>\r\n        <div class=\"boxform\">\r\n          <label for=\"colaborador\" class=\"colaborador\">Etiquetas</label>\r\n         \r\n\r\n            <div>\r\n              \r\n              <div class=\"bdy_tbl\">\r\n                 <div v-for=\"(item, index) in added.etiquetas\" :key=\"index\">\r\n                  <p  @click=\"delEtiqueta(index)\">{{ item}} </p>\r\n                 </div>\r\n              </div>\r\n            </div>\r\n\r\n           \r\n            <input\r\n              v-model=\"etiqueta\"\r\n              type=\"text\"\r\n              :placeholder=\"'etiqueta'\"\r\n              class=\"inp_buscar\"\r\n            />\r\n          <div class=\"box_rh\">\r\n            <p :class=\"{'active_option' : etiqueta}\" @click=\"addEtiqueta(etiqueta)\" class=\"option_btn2\">Añadir</p>\r\n          </div>\r\n        </div>\r\n        \r\n      </div>\r\n     \r\n     \r\n      <div class=\"grup-form dflx\">\r\n          <div class=\"boxform\">\r\n            <label for=\"ubicacion\" class=\"ubicacion\">Zona </label>\r\n            <input\r\n              v-model=\"added.ubicacion\"\r\n              type=\"text\"\r\n              id=\"ubicacion\"\r\n              name=\"ubicacion\"\r\n            />\r\n          </div>\r\n          <div class=\"boxform\">\r\n            <label for=\"precio\" class=\"precio\">Precio </label>\r\n            <input\r\n              v-model=\"added.precio\"\r\n               type=\"number\"\r\n              id=\"precio\"\r\n              name=\"precio\" \r\n              min=\"0\" \r\n              step=\"any\" \r\n              required\r\n            />\r\n          </div>\r\n          <div class=\"boxform\">\r\n            <label for=\"superficie\" class=\"superficie\">Superficie</label>\r\n            <input\r\n              v-model=\"added.superficie\"\r\n              type=\"text\"\r\n              id=\"superficie\"\r\n              name=\"superficie\"\r\n            />\r\n          </div>\r\n        \r\n        \r\n          \r\n        </div>\r\n      \r\n\r\n       \r\n\r\n\r\n       <!-- <div class=\"grup-form dflx\">\r\n          <div class=\"boxform\">\r\n            <label for=\"iframe\" class=\"iframe\">Mapa iframe (opcional) </label>\r\n            <input\r\n              v-model=\"iframe\"\r\n              type=\"text\"\r\n              id=\"iframe\"\r\n              name=\"iframe\"\r\n              class=\"wd-lrg\"\r\n            />\r\n          </div>\r\n         \r\n          \r\n        </div> -->\r\n\r\n\r\n        <div class=\"grup-form dflx\">\r\n        \r\n        <div class=\"boxform\">\r\n          <label >Estatus</label>\r\n          <div class=\"options_btn\">\r\n              <p @click=\"added.estatus = 'ACTIVO'\" class=\"option_btn\" :class=\"{'active_option' : added.estatus == 'ACTIVO'}\">VISIBLE</p>\r\n              <p @click=\"added.estatus = 'INACTIVO'\" class=\"option_btn\" :class=\"{'active_option' : added.estatus == 'INACTIVO'}\">OCULTO</p>\r\n          </div>\r\n        </div>\r\n        <div class=\"boxform\">\r\n          <label >Tipo</label>\r\n          <div class=\"options_btn\">\r\n              <p @click=\"added.tipo = 'venta'\" class=\"option_btn\" :class=\"{'active_option' : added.tipo == 'venta'}\">VENTA</p>\r\n              <p @click=\"added.tipo = 'renta'\" class=\"option_btn\" :class=\"{'active_option' : added.tipo == 'renta'}\">RENTA</p>\r\n          </div>\r\n        </div>\r\n        <div class=\"boxform\" v-if=\"added.tipo == 'venta'\">\r\n          <label >Opción a rentar</label>\r\n          <div class=\"options_btn\">\r\n              <p @click=\"added.renta = 'si'\" class=\"option_btn\" :class=\"{'active_option' : added.renta == 'si'}\">SI</p>\r\n              <p @click=\"added.renta = 'no'\" class=\"option_btn\" :class=\"{'active_option' : added.renta == 'no'}\">NO</p>\r\n          </div>\r\n        </div>\r\n        \r\n      </div>\r\n\r\n      \r\n      <div class=\"content-vue-editor\">\r\n        <vue-editor\r\n                 id=\"editor\"\r\n                 class=\"quill-editor\"\r\n                :editorToolbar=\"toolbar()\"\r\n                v-model=\"added.detalles\"\r\n                >\r\n        </vue-editor> \r\n        <div class=\"box_rh\">\r\n            <p  @click=\"added.detalles =''\" class=\"option_btn2\">Borrar</p>\r\n         </div>\r\n      </div>\r\n    \r\n       \r\n\r\n      \r\n\r\n        <div class=\"dflx\">\r\n          <p @click=\"wait()\" class=\"btnRegresar\">Regresar</p>\r\n          <button class=\"alta\" type=\"submit\">Editar</button>\r\n        </div>\r\n\r\n        <div v-if=\"status != ''\" class=\"status_messages\">\r\n          <div v-if=\"status == 'success'\" class=\"msg msg_success\">\r\n            <p>{{ message }}</p>\r\n          </div>\r\n          <div v-if=\"status == 'error'\" class=\"msg msg_error\">\r\n            <p>{{ message }}</p>\r\n          </div>\r\n        </div>\r\n      </form>\r\n    </div>\r\n\r\n\r\n\r\n\r\n    <div class=\"seccion_div\">\r\n        <p class=\"seccion_subtitle\">Fotos</p>\r\n    </div>\r\n\r\n    <form\r\n        @submit.prevent=\"\r\n          submitFiles(files)\r\n        \"\r\n      >\r\n       \r\n     \r\n     \r\n\r\n        <div class=\"form-img\">\r\n      <div class=\"grup-form dflx\">\r\n        <div class=\"boxform\">\r\n        \r\n          <div class=\"dflx\">\r\n            <label class=\"filelabel\" for=\"uploadimg\">\r\n              <span>Seleccionar Archivo</span></label\r\n            >\r\n           \r\n          </div>\r\n\r\n          <input\r\n            @change=\"previewFiles\"\r\n            type=\"file\"\r\n            id=\"uploadimg\"\r\n            name=\"uploadimg\"\r\n            \r\n            class=\"fileinput\" ref=\"myFiles\"  multiple\r\n          />\r\n        </div>\r\n\r\n       \r\n      </div>\r\n\r\n\r\n    </div>\r\n    <div class=\"grup-form dflx\">\r\n      <div  class=\"boxform\">\r\n\r\n        \r\n              \r\n<div class=\"bdy_tbl wd-md\">\r\n   <div v-for=\"(img, key) in files\" :key=\"key\">\r\n    <p >{{ img.name }}</p>\r\n   </div>\r\n</div>\r\n\r\n</div>\r\n    </div>\r\n\r\n\r\n       \r\n\r\n    <div v-if=\"status != ''\" class=\"status_messages\">\r\n          <div v-if=\"status == 'success'\" class=\"msg msg_success\">\r\n            <p>{{ message }}</p>\r\n          </div>\r\n          <div v-if=\"status == 'error'\" class=\"msg msg_error\">\r\n            <p>{{ message }}</p>\r\n          </div>\r\n        </div>\r\n     \r\n\r\n        <div class=\"dflx\">\r\n         \r\n          <button class=\"alta\" type=\"submit\">Añadir</button>\r\n        </div>\r\n\r\n     \r\n      </form>\r\n\r\n\r\n    <div class=\"tbl_fotos\">\r\n        <p class=\"psin\" v-if=\"added.fotos.length ==0\">Esta propiedad aun no tiene fotos añadidas.</p>\r\n        <div class=\"bdy_foto\" v-for=\"(item, index) in added.fotos\" :key=\"index\" >\r\n          <img :src=\"url+'locales-img/'+item.image\" alt=\"\">\r\n        <div class=\"dflx\">\r\n          <div class=\"box_ctr\">\r\n            <p  @click=\"updatePortada(item.id , added.id)\" :class=\"{'active_option' : item.portada =='si'}\" class=\"option_btn2\">Portada</p>\r\n         </div>\r\n          <div class=\"box_ctr\">\r\n            <p @click=\"deleteFoto(item.id)\"  class=\"option_btn2\">Borrar</p>\r\n         </div>\r\n        </div>\r\n         \r\n        \r\n        </div>\r\n    </div>\r\n\r\n\r\n\r\n\r\n\r\n   \r\n\r\n  \r\n\r\n    <div v-if=\"viewModal\" class=\"modal_view\">\r\n      <div id=\"modal_edit\">\r\n        <div class=\"body dflx\">\r\n          <h3>Local actualizado</h3>\r\n          <img src=\"../../assets/default/add.png\" alt=\"icono alerta\" />\r\n        </div>\r\n\r\n        <div class=\"modal_edit_btns dflx\">\r\n          <p @click=\"wait()\" class=\"otro\">Aceptar</p>\r\n        \r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\n\r\nimport { mapActions } from \"vuex\";\r\nimport {url} from '../../global/index'; \r\nimport { VueEditor } from \"vue2-editor\";\r\n\r\nexport default {\r\n  name: \"EditLcl\",\r\n  components: {\r\n    VueEditor,\r\n  },\r\n  data() {\r\n    return {\r\n      status: \"\",\r\n      message: \"\",\r\n      status2: \"\",\r\n      message2: \"\",\r\n      active: false,\r\n      url:url,\r\n      archivo: \"No se eligió archivo\",\r\n      statusimg:false,\r\n      //MODAL\r\n      file1: null,\r\n      viewModal: false,\r\n      search_value:'',\r\n      etiqueta:'',\r\n      files:[]\r\n    };\r\n  },\r\n \r\n  async created() {\r\n    \r\n\r\n       await this.getAllInfoClb('colaborador')\r\n  },\r\n  computed: {\r\n    added() {\r\n      return this.$store.getters[\"locales/getAdded\"];\r\n    },\r\n    identity() {\r\n        return this.$store.getters[\"admin/getIdentity\"];\r\n      },\r\n    colaboradores() {\r\n         let data = this.$store.getters[\"colaborador/data\"];\r\n            \r\n          if(!Array.isArray(data)) return []\r\n          if(this.identity.role =='colaborador')data = data.filter(item => item.id == this.identity.sub);\r\n        if(this.search_value !=''){\r\n         \r\n          let filt = data.filter(item => {\r\n            const fullname = item.name +' '+ item.lastname;\r\n            return this.includesItem(this.search_value, fullname ) \r\n          })\r\n          data = filt\r\n        }\r\n\r\n        return data\r\n      },\r\n    data() {\r\n      return this.$store.getters[\"locales/data\"];\r\n    },\r\n  },\r\n  methods: {\r\n    ...mapActions(\"locales\", [\"setAddedLcl\"]),\r\n    ...mapActions(\"locales\", [\"getAllInfoClb\"]),\r\n    ...mapActions(\"locales\", [\"getInfoByIdLcl\"]),\r\n    ...mapActions(\"colaborador\", [\"getAllInfoClb\"]),\r\n\r\n    \r\n    updateAdded: async function (){\r\n        if(!this.added.id) return setTimeout(() => this.$router.go(), 200); \r\n       \r\n        const result =await  this.getInfoByIdLcl({\r\n          id:this.added.id,\r\n          option:'locales'\r\n        })\r\n      \r\n        if(result.id){\r\n          this.setAddedLcl(result)\r\n        }\r\n    },\r\n    addEtiqueta(value){\r\n        if(value){\r\n          let newarr =[]\r\n          for (const item of this.added.etiquetas) {\r\n              newarr.push(item)\r\n          }  \r\n          newarr.push(value)\r\n          this.added.etiquetas = newarr\r\n          this.etiqueta =''\r\n        }\r\n    },\r\n    delEtiqueta(index){\r\n\r\n    \r\n        let newarr =[]\r\n        for(var i =0; i<this.added.etiquetas.length; i++){\r\n          if(i!=index) newarr.push(this.added.etiquetas[i]);\r\n        }\r\n        this.added.etiquetas = newarr\r\n      \r\n\r\n    },\r\n  \r\n    includesItem(search, name){\r\n            name = name.normalize(\"NFD\").replace(/[\\u0300-\\u036f]/g, \"\");\r\n          if(search ==''){\r\n            return true\r\n          }\r\n          name = name.toLowerCase()\r\n          search= search.toLowerCase()\r\n          if(search ==''){\r\n              return false;\r\n          }\r\n          return name.includes(search)\r\n        },\r\n\r\n\r\n    wait: function () {\r\n      setTimeout(() => this.$router.go(), 200);\r\n    },\r\n    ...mapActions(\"locales\", [\"getInfoByIdLcl\"]),\r\n    submit: async function ( id_colaborador , titulo , estatus, renta, tipo, descripcion, ubicacion, precio, superficie, iframe, detalles, etiquetas) {\r\n      this.status = \"\";\r\n      this.message = \"\";\r\n   \r\n       \r\n\r\n        let result = await this.$store.dispatch(\"locales/editItemLcl\", {\r\n          option: \"locales\",\r\n          item: { id: this.added.id,  id_colaborador , titulo , estatus, renta, tipo, descripcion, ubicacion, precio, superficie, iframe, detalles, etiquetas},\r\n        });\r\n\r\n        if (result.status == \"error\") {\r\n          this.status = \"error\";\r\n          this.message = result.message;\r\n        } else {\r\n          // success\r\n          this.showModal();\r\n        }\r\n     \r\n    },\r\n    updatePortada: async function (id , id_local){\r\n      let result = await this.$store.dispatch(\"locales/editItemLcl\", {\r\n          option: \"locales/portada\",\r\n          item: { id , id_local},\r\n        });\r\n\r\n        if (result.status == \"error\") {\r\n          this.status = \"error\";\r\n          this.message = result.message;\r\n        } else {\r\n          // success\r\n          this.updateAdded()\r\n        }\r\n    },  \r\n    deleteFoto: async function (id){\r\n  \r\n            this.status='';\r\n            this.message='';\r\n\r\n             let result = await this.$store.dispatch(\"locales/deleteItemLcl\", {option: 'locales/foto', id:id});\r\n           \r\n            if(result.status=='error'){\r\n              this.message = result.message;\r\n              this.status='error'\r\n              this.delStatus()\r\n           \r\n            }else{\r\n                this.status='';\r\n                this.message='';\r\n                this.updateAdded()\r\n            }\r\n    },\r\n    submitFiles: async function ( files) {\r\n      this.status = \"\";\r\n      this.message = \"\";\r\n   \r\n\r\n     if(files.length>0){\r\n\r\n         const id = this.added.id;\r\n\r\n          for (const image of files) {\r\n                //Creamos el formData\r\n                var data = new FormData();\r\n                //Añadimos la imagen seleccionada\r\n                data.append(\"image\", image);\r\n                data.append(\"id\", id);\r\n\r\n                //Añadimos el método PUT dentro del formData\r\n                // Como lo hacíamos desde un formulario simple _(no ajax)_\r\n                data.append(\"_method\", \"POST\");\r\n\r\n                 await this.$store.dispatch(\"locales/addItemLcl\", {\r\n                  option: \"locales/foto\",\r\n                  item: data,\r\n                });\r\n          }\r\n\r\n\r\n\r\n           // success\r\n           this.files=[]\r\n           this.updateAdded()\r\n\r\n       \r\n\r\n     }else{\r\n          this.status = \"error\";\r\n          this.message = 'No has adjuntado ninguna foto.';\r\n          this.delStatus()\r\n     }\r\n  \r\n\r\n        \r\n      \r\n    },\r\n  \r\n    previewFiles() {\r\n    this.files = this.$refs.myFiles.files\r\n  },\r\n  delStatus: function () {\r\n      setTimeout(() => this.delMsgs(), 2000);\r\n    },\r\n    delMsgs: function () {\r\n      this.status = \"\";\r\n      this.message = \"\";\r\n    },\r\n\r\n    showModal: function () {\r\n      this.viewModal = true;\r\n    },\r\n    \r\n  \r\n    toolbar:function(){\r\n      return  [\r\n                ['bold', 'italic', 'underline', 'strike'],        // toggled buttons\r\n                ['blockquote', 'code-block'],\r\n\r\n                [{ 'header': 1 }, { 'header': 2 }],               // custom button values\r\n                [{ 'list': 'ordered'}, { 'list': 'bullet' }],\r\n                [{ 'script': 'sub'}, { 'script': 'super' }],      // superscript/subscript\r\n                [{ 'indent': '-1'}, { 'indent': '+1' }],          // outdent/indent\r\n                [{ 'direction': 'rtl' }],                         // text direction\r\n\r\n                [{ 'size': ['small', false, 'large', 'huge'] }],  // custom dropdown\r\n                [{ 'header': [1, 2, 3, 4, 5, 6, false] }],\r\n               // [ 'link','image' ,'video' ],          // add's image support\r\n                [{ 'color': [] }, { 'background': [] }],          // dropdown with defaults from theme\r\n                [{ 'font': [] }],\r\n                [{ 'align': [] }],\r\n\r\n                ['clean']                                         // remove formatting button\r\n            ];\r\n     },\r\n  \r\n\r\n   \r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n#EditLcl label {\r\n  color: var(--color-2);\r\n  font-weight: 700;\r\n  font-size: 0.7291666666666666vw;\r\n  margin-bottom: 0.3vw;\r\n}\r\n\r\n#EditLcl input,\r\n#EditLcl select {\r\n  height: 2.5vw;\r\n  width: 14.716666666666668vw;\r\n  border: 0.052083vw solid var(--color-2);\r\n  border-radius: 0vw 1.5vw 1.5vw 0vw;\r\n  color: var(--color-3);\r\n  font-size: 0.7291666666666666vw;\r\n  font-weight: 500;\r\n  padding-left: 1.09375vw;\r\n}\r\n\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}