{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapActions } from \"vuex\";\nimport { VueEditor } from \"vue2-editor\";\nexport default {\n  name: \"AddLcl\",\n  components: {\n    VueEditor\n  },\n  data() {\n    return {\n      status: \"\",\n      message: \"\",\n      id_colaborador: \"\",\n      colaborador: \"\",\n      titulo: \"\",\n      estatus: \"ACTIVO\",\n      renta: \"no\",\n      tipo: \"venta\",\n      descripcion: \"\",\n      ubicacion: \"\",\n      precio: 0,\n      superficie: \"\",\n      iframe: \"\",\n      detalles: \"\",\n      etiquetas: [],\n      etiqueta: \"\",\n      act_precio: \"ACTIVO\",\n      leyenda: \"\",\n      tipov: \"imagen\",\n      video_link: \"\",\n      //MODAL\n      files: [],\n      viewModal: false,\n      search_value: \"\"\n    };\n  },\n  async created() {\n    await this.getAllInfoClb(\"colaborador\");\n  },\n  computed: {\n    colaboradores() {\n      let data = this.$store.getters[\"colaborador/data\"];\n      if (!Array.isArray(data)) return [];\n      if (this.identity.role == \"colaborador\") data = data.filter(item => item.id == this.identity.sub);\n      if (this.search_value != \"\") {\n        let filt = data.filter(item => {\n          const fullname = item.name + \" \" + item.lastname;\n          return this.includesItem(this.search_value, fullname);\n        });\n        data = filt;\n      }\n      return data;\n    },\n    identity() {\n      return this.$store.getters[\"admin/getIdentity\"];\n    }\n  },\n  methods: {\n    ...mapActions(\"locales\", [\"setAddedLcl\"]),\n    ...mapActions(\"locales\", [\"setHistoryOptionLcl\"]),\n    ...mapActions(\"colaborador\", [\"getAllInfoClb\"]),\n    ...mapActions(\"locales\", [\"getInfoByIdLcl\"]),\n    previewFiles() {\n      this.files = this.$refs.myFiles.files;\n    },\n    addEtiqueta(value) {\n      if (value) {\n        let newarr = [];\n        for (const item of this.etiquetas) {\n          newarr.push(item);\n        }\n        newarr.push(value);\n        this.etiquetas = newarr;\n        this.etiqueta = \"\";\n      }\n    },\n    delEtiqueta(index) {\n      let newarr = [];\n      for (var i = 0; i < this.etiquetas.length; i++) {\n        if (i != index) newarr.push(this.etiquetas[i]);\n      }\n      this.etiquetas = newarr;\n    },\n    wait: function () {\n      setTimeout(() => this.$router.go(), 200);\n    },\n    includesItem(search, name) {\n      name = name.normalize(\"NFD\").replace(/[\\u0300-\\u036f]/g, \"\");\n      if (search == \"\") {\n        return true;\n      }\n      name = name.toLowerCase();\n      search = search.toLowerCase();\n      if (search == \"\") {\n        return false;\n      }\n      return name.includes(search);\n    },\n    submit: async function (id_colaborador, titulo, estatus, renta, tipo, descripcion, ubicacion, precio, superficie, iframe, detalles, etiquetas, act_precio, leyenda, files, tipov, video_link) {\n      this.status = \"\";\n      this.message = \"\";\n      if (tipov == \"imagen\") {\n        if (files.length > 0) {\n          let result = await this.$store.dispatch(\"locales/addItemLcl\", {\n            option: \"locales\",\n            item: {\n              id_colaborador,\n              titulo,\n              estatus,\n              renta,\n              tipo,\n              descripcion,\n              ubicacion,\n              precio,\n              superficie,\n              iframe,\n              detalles,\n              etiquetas,\n              act_precio,\n              leyenda\n            }\n          });\n          if (result.status == \"error\") {\n            this.status = \"error\";\n            this.message = result.message;\n          } else {\n            // success\n            const id = result.added.id;\n            for (const image of files) {\n              //Creamos el formData\n              var data = new FormData();\n              //Añadimos la imagen seleccionada\n              data.append(\"image\", image);\n              data.append(\"video_link\", video_link);\n              data.append(\"tipov\", tipov);\n              data.append(\"id\", id);\n\n              //Añadimos el método PUT dentro del formData\n              // Como lo hacíamos desde un formulario simple _(no ajax)_\n              data.append(\"_method\", \"POST\");\n              await this.$store.dispatch(\"locales/addItemLcl\", {\n                option: \"locales/foto\",\n                item: data\n              });\n            }\n            const newadded = await this.getInfoByIdLcl({\n              id: result.added.id,\n              option: \"locales\"\n            });\n            this.files = [];\n            this.setAddedLcl(newadded);\n            this.showModal();\n          }\n        } else {\n          this.status = \"error\";\n          this.message = \"No has adjuntado ninguna foto.\";\n          this.delStatus();\n        }\n      } else if (tipov == \"video\") {\n        // success\n        const id = result.added.id;\n        const image = \"\";\n        //Creamos el formData\n        /**/\n        var data = new FormData();\n        data.append(\"image\", \"\");\n        data.append(\"video_link\", video_link);\n        data.append(\"tipov\", tipov);\n        data.append(\"id\", id);\n        data.append(\"_method\", \"POST\");\n        await this.$store.dispatch(\"locales/addItemLcl\", {\n          option: \"locales/foto\",\n          item: data\n        });\n        const newadded = await this.getInfoByIdLcl({\n          id: result.added.id,\n          option: \"locales\"\n        });\n        this.files = [];\n        this.setAddedLcl(newadded);\n        this.showModal();\n      }\n    },\n    delStatus: function () {\n      setTimeout(() => this.delMsgs(), 2000);\n    },\n    delMsgs: function () {\n      this.status = \"\";\n      this.message = \"\";\n    },\n    showModal: function () {\n      this.viewModal = true;\n    },\n    Add: function () {\n      this.status = \"\";\n      this.message = \"\";\n      this.id_colaborador = \"\";\n      this.titulo = \"\";\n      this.estatus = \"\";\n      this.renta = \"no\";\n      this.tipo = \"venta\";\n      this.descripcion = \"\";\n      this.ubicacion = \"\";\n      this.precio = \"\";\n      this.superficie = \"\";\n      this.iframe = \"\";\n      this.detalles = \"\";\n      this.etiquetas = [];\n      this.viewModal = false;\n      this.setAddedLcl(\"\");\n    },\n    toEdit: function () {\n      this.setHistoryOptionLcl(\"Edit\");\n      this.viewModal = false;\n    },\n    back: function () {\n      setTimeout(() => this.$router.go(), 200);\n    },\n    toolbar: function () {\n      return [[\"bold\", \"italic\", \"underline\", \"strike\"],\n      // toggled buttons\n      [\"blockquote\", \"code-block\"], [{\n        header: 1\n      }, {\n        header: 2\n      }],\n      // custom button values\n      [{\n        list: \"ordered\"\n      }, {\n        list: \"bullet\"\n      }], [{\n        script: \"sub\"\n      }, {\n        script: \"super\"\n      }],\n      // superscript/subscript\n      [{\n        indent: \"-1\"\n      }, {\n        indent: \"+1\"\n      }],\n      // outdent/indent\n      [{\n        direction: \"rtl\"\n      }],\n      // text direction\n\n      [{\n        size: [\"small\", false, \"large\", \"huge\"]\n      }],\n      // custom dropdown\n      [{\n        header: [1, 2, 3, 4, 5, 6, false]\n      }],\n      // [ 'link','image' ,'video' ],          // add's image support\n      [{\n        color: []\n      }, {\n        background: []\n      }],\n      // dropdown with defaults from theme\n      [{\n        font: []\n      }], [{\n        align: []\n      }], [\"clean\"] // remove formatting button\n      ];\n    },\n\n    toolbarvideo: function () {\n      return [[\"video\"]];\n    }\n  }\n};","map":{"version":3,"mappings":";AAsWA;AACA;AACA;EACAA;EACAC;IACAC;EACA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MAEAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACA;MACAC;MACAC;MAEAC;IACA;EACA;EACA;IACA;EACA;EACAC;IACAC;MACA;MAEA;MACA,yCACAzB;MAEA;QACA;UACA;UACA;QACA;QACAA;MACA;MAEA;IACA;IACA0B;MACA;IACA;EACA;EACAC;IACA;IACA;IACA;IACA;IACAC;MACA;IACA;IACAC;MACA;QACA;QACA;UACAC;QACA;QACAA;QACA;QACA;MACA;IACA;IACAC;MACA;MACA;QACA;MACA;MACA;IACA;IACAC;MACAC;IACA;IACAC;MACArC;MACA;QACA;MACA;MACAA;MACAsC;MACA;QACA;MACA;MACA;IACA;IACAC,wBACAjC,gBACAE,QACAC,SACAC,OACAC,MACAC,aACAC,WACAC,QACAC,YACAC,QACAC,UACAC,WACAE,YACAC,SACAG,OACAF,OACAC,YACA;MACA;MACA;MAEA;QACA;UACA;YACAiB;YACAC;cACAnC;cACAE;cACAC;cACAC;cACAC;cACAC;cACAC;cACAC;cACAC;cACAC;cACAC;cACAC;cACAE;cACAC;YACA;UACA;UAEA;YACA;YACA;UACA;YACA;YACA;YAEA;cACA;cACA;cACA;cACAlB;cACAA;cACAA;cACAA;;cAEA;cACA;cACAA;cAEA;gBACAqC;gBACAC;cACA;YACA;YAEA;cACAC;cACAF;YACA;YACA;YACA;YACA;UACA;QACA;UACA;UACA;UACA;QACA;MACA;QACA;QACA;QACA;QACA;QACA;QAAA;QACArC;QACAA;QACAA;QACAA;QACAA;QAEA;UACAqC;UACAC;QACA;QAEA;UACAC;UACAF;QACA;QACA;QACA;QACA;MACA;IACA;IACAG;MACAP;IACA;IACAQ;MACA;MACA;IACA;IAEAC;MACA;IACA;IAEAC;MACA;MACA;MAEA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MAEA;MACA;IACA;IACAC;MACA;MACA;IACA;IACAC;MACAZ;IACA;IAEAa;MACA,QACA;MAAA;MACA,8BAEA;QAAAC;MAAA;QAAAA;MAAA;MAAA;MACA;QAAAC;MAAA;QAAAA;MAAA,IACA;QAAAC;MAAA;QAAAA;MAAA;MAAA;MACA;QAAAC;MAAA;QAAAA;MAAA;MAAA;MACA;QAAAC;MAAA;MAAA;;MAEA;QAAAC;MAAA;MAAA;MACA;QAAAL;MAAA;MACA;MACA;QAAAM;MAAA;QAAAC;MAAA;MAAA;MACA;QAAAC;MAAA,IACA;QAAAC;MAAA,IAEA;MAAA,CACA;IACA;;IACAC;MACA;IACA;EACA;AACA","names":["name","components","VueEditor","data","status","message","id_colaborador","colaborador","titulo","estatus","renta","tipo","descripcion","ubicacion","precio","superficie","iframe","detalles","etiquetas","etiqueta","act_precio","leyenda","tipov","video_link","files","viewModal","search_value","computed","colaboradores","identity","methods","previewFiles","addEtiqueta","newarr","delEtiqueta","wait","setTimeout","includesItem","search","submit","option","item","id","delStatus","delMsgs","showModal","Add","toEdit","back","toolbar","header","list","script","indent","direction","size","color","background","font","align","toolbarvideo"],"sourceRoot":"src/views/locales","sources":["addLcl.vue"],"sourcesContent":["<template>\r\n  <div id=\"AddLcl\">\r\n    <div class=\"form-content\">\r\n      <form\r\n        @submit.prevent=\"\r\n          submit(\r\n            id_colaborador,\r\n            titulo,\r\n            estatus,\r\n            renta,\r\n            tipo,\r\n            descripcion,\r\n            ubicacion,\r\n            precio,\r\n            superficie,\r\n            iframe,\r\n            detalles,\r\n            etiquetas,\r\n            act_precio,\r\n            leyenda,\r\n            files\r\n          )\r\n        \"\r\n      >\r\n        <div class=\"grup-form dflx\">\r\n          <div class=\"boxform\">\r\n            <label for=\"titulo\" class=\"titulo\">Título </label>\r\n            <input\r\n              v-model=\"titulo\"\r\n              type=\"text\"\r\n              id=\"titulo\"\r\n              name=\"titulo\"\r\n              class=\"wd-lrg\"\r\n              required\r\n            />\r\n          </div>\r\n        </div>\r\n\r\n        <!--  <div class=\"grup-form dflx\">\r\n        \r\n        <div class=\"boxform\">\r\n          <label for=\"descripcion\" class=\"descripcion\">Descripción</label>\r\n          <textarea\r\n            v-model=\"descripcion\"\r\n            class=\"format_ta\"\r\n            id=\"descripcion\"\r\n            name=\"descripcion\"\r\n          />\r\n        </div>\r\n     \r\n        \r\n      </div> -->\r\n        <div class=\"grup-form dflx\">\r\n          <div class=\"boxform\">\r\n            <label for=\"colaborador\" class=\"colaborador\">Colaborador</label>\r\n            <input\r\n              v-model=\"colaborador\"\r\n              type=\"text\"\r\n              id=\"colaborador\"\r\n              name=\"colaborador\"\r\n              required\r\n              disabled\r\n            />\r\n\r\n            <div>\r\n              <div class=\"bdy_tbl\">\r\n                <div v-for=\"(item, index) in colaboradores\" :key=\"index\">\r\n                  <p\r\n                    :class=\"{ active_item: id_colaborador == item.id }\"\r\n                    @click=\"\r\n                      (id_colaborador = item.id),\r\n                        (colaborador = item.name + ' ' + item.lastname)\r\n                    \"\r\n                  >\r\n                    {{ item.name }} {{ item.lastname }}\r\n                  </p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <input\r\n              v-model=\"search_value\"\r\n              type=\"text\"\r\n              :placeholder=\"'Buscar'\"\r\n              class=\"inp_buscar\"\r\n            />\r\n          </div>\r\n          <div class=\"boxform\">\r\n            <label for=\"colaborador\" class=\"colaborador\">Etiquetas</label>\r\n\r\n            <div>\r\n              <div class=\"bdy_tbl\">\r\n                <div v-for=\"(item, index) in etiquetas\" :key=\"index\">\r\n                  <p @click=\"delEtiqueta(index)\">{{ item }}</p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <input\r\n              v-model=\"etiqueta\"\r\n              type=\"text\"\r\n              :placeholder=\"'etiqueta'\"\r\n              class=\"inp_buscar\"\r\n            />\r\n            <div class=\"box_rh\">\r\n              <p\r\n                :class=\"{ active_option: etiqueta }\"\r\n                @click=\"addEtiqueta(etiqueta)\"\r\n                class=\"option_btn2\"\r\n              >\r\n                Añadir\r\n              </p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"grup-form dflx\">\r\n          <div class=\"boxform\">\r\n            <label>Activar precio</label>\r\n            <div class=\"options_btn\">\r\n              <p\r\n                @click=\"act_precio == 'ACTIVO'\"\r\n                class=\"option_btn\"\r\n                :class=\"{ active_option: act_precio == 'ACTIVO' }\"\r\n              >\r\n                ACTIVAR\r\n              </p>\r\n              <p\r\n                @click=\"act_precio = 'INACTIVO'\"\r\n                class=\"option_btn\"\r\n                :class=\"{ active_option: act_precio == 'INACTIVO' }\"\r\n              >\r\n                DESACTIVAR\r\n              </p>\r\n            </div>\r\n          </div>\r\n          <div class=\"boxform\" v-if=\"act_precio == 'ACTIVO'\">\r\n            <label for=\"precio\" class=\"precio\">Precio </label>\r\n            <input\r\n              v-model=\"precio\"\r\n              type=\"number\"\r\n              id=\"precio\"\r\n              name=\"precio\"\r\n              min=\"0\"\r\n              step=\"any\"\r\n            />\r\n          </div>\r\n          <div class=\"boxform\" v-if=\"act_precio == 'INACTIVO'\">\r\n            <label for=\"ubicacion\" class=\"ubicacion\">Leyenda </label>\r\n            <input\r\n              v-model=\"leyenda\"\r\n              type=\"text\"\r\n              id=\"ubicacion\"\r\n              name=\"ubicacion\"\r\n            />\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"grup-form dflx\">\r\n          <div class=\"boxform\">\r\n            <label for=\"ubicacion\" class=\"ubicacion\">Zona </label>\r\n            <input\r\n              v-model=\"ubicacion\"\r\n              type=\"text\"\r\n              id=\"ubicacion\"\r\n              name=\"ubicacion\"\r\n            />\r\n          </div>\r\n          <div class=\"boxform\">\r\n            <label for=\"superficie\" class=\"superficie\">Superficie</label>\r\n            <input\r\n              v-model=\"superficie\"\r\n              type=\"text\"\r\n              id=\"superficie\"\r\n              name=\"superficie\"\r\n            />\r\n          </div>\r\n        </div>\r\n\r\n        <!-- <div class=\"grup-form dflx\">\r\n          <div class=\"boxform\">\r\n            <label for=\"iframe\" class=\"iframe\">Mapa iframe (opcional) </label>\r\n            <input\r\n              v-model=\"iframe\"\r\n              type=\"text\"\r\n              id=\"iframe\"\r\n              name=\"iframe\"\r\n              class=\"wd-lrg\"\r\n            />\r\n          </div>\r\n         \r\n          \r\n        </div> -->\r\n\r\n        <div class=\"grup-form dflx\">\r\n          <div class=\"boxform\">\r\n            <label>Estatus</label>\r\n            <div class=\"options_btn\">\r\n              <p\r\n                @click=\"estatus = 'ACTIVO'\"\r\n                class=\"option_btn\"\r\n                :class=\"{ active_option: estatus == 'ACTIVO' }\"\r\n              >\r\n                VISIBLE\r\n              </p>\r\n              <p\r\n                @click=\"estatus = 'INACTIVO'\"\r\n                class=\"option_btn\"\r\n                :class=\"{ active_option: estatus == 'INACTIVO' }\"\r\n              >\r\n                OCULTO\r\n              </p>\r\n            </div>\r\n          </div>\r\n          <div class=\"boxform\">\r\n            <label>Tipo</label>\r\n            <div class=\"options_btn\">\r\n              <p\r\n                @click=\"tipo = 'venta'\"\r\n                class=\"option_btn\"\r\n                :class=\"{ active_option: tipo == 'venta' }\"\r\n              >\r\n                VENTA\r\n              </p>\r\n              <p\r\n                @click=\"tipo = 'renta'\"\r\n                class=\"option_btn\"\r\n                :class=\"{ active_option: tipo == 'renta' }\"\r\n              >\r\n                RENTA\r\n              </p>\r\n            </div>\r\n          </div>\r\n          <div class=\"boxform\" v-if=\"tipo == 'venta'\">\r\n            <label>Opción a rentar</label>\r\n            <div class=\"options_btn\">\r\n              <p\r\n                @click=\"renta = 'si'\"\r\n                class=\"option_btn\"\r\n                :class=\"{ active_option: renta == 'si' }\"\r\n              >\r\n                SI\r\n              </p>\r\n              <p\r\n                @click=\"renta = 'no'\"\r\n                class=\"option_btn\"\r\n                :class=\"{ active_option: renta == 'no' }\"\r\n              >\r\n                NO\r\n              </p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <vue-editor\r\n          id=\"editor\"\r\n          class=\"quill-editor\"\r\n          :editorToolbar=\"toolbar()\"\r\n          v-model=\"detalles\"\r\n        >\r\n        </vue-editor>\r\n\r\n        <div class=\"boxform\">\r\n          <label>Imagen/Video</label>\r\n          <div class=\"options_btn\">\r\n            <p\r\n              @click=\"tipov = 'imagen'\"\r\n              class=\"option_btn\"\r\n              :class=\"{ active_option: tipov == 'imagen' }\"\r\n            >\r\n              IMAGEN\r\n            </p>\r\n            <p\r\n              @click=\"tipov = 'video'\"\r\n              class=\"option_btn\"\r\n              :class=\"{ active_option: tipov == 'video' }\"\r\n            >\r\n              VIDEO\r\n            </p>\r\n          </div>\r\n        </div>\r\n\r\n        <section v-if=\"tipov == 'imagen'\">\r\n          <div class=\"form-img\">\r\n            <div class=\"grup-form dflx\">\r\n              <div class=\"boxform\">\r\n                <label>Fotos</label>\r\n                <div class=\"dflx\">\r\n                  <label class=\"filelabel\" for=\"uploadimg\">\r\n                    <span>Seleccionar Archivo</span></label\r\n                  >\r\n                </div>\r\n\r\n                <input\r\n                  @change=\"previewFiles\"\r\n                  type=\"file\"\r\n                  id=\"uploadimg\"\r\n                  name=\"uploadimg\"\r\n                  class=\"fileinput\"\r\n                  ref=\"myFiles\"\r\n                  multiple\r\n                />\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"grup-form dflx\">\r\n            <div class=\"boxform\">\r\n              <div class=\"bdy_tbl wd-md\">\r\n                <div v-for=\"(img, key) in files\" :key=\"key\">\r\n                  <p>{{ img.name }}</p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </section>\r\n        <section v-if=\"tipov == 'video'\">\r\n          <vue-editor\r\n            id=\"editor\"\r\n            class=\"quill-editor\"\r\n            :editorToolbar=\"toolbarvideo()\"\r\n            v-model=\"video_link\"\r\n          >\r\n          </vue-editor>\r\n        </section>\r\n\r\n        <div v-if=\"status != ''\" class=\"status_messages\">\r\n          <div v-if=\"status == 'success'\" class=\"msg msg_success\">\r\n            <p>{{ message }}</p>\r\n          </div>\r\n          <div v-if=\"status == 'error'\" class=\"msg msg_error\">\r\n            <p>{{ message }}</p>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"dflx\">\r\n          <p @click=\"wait()\" class=\"btnRegresar\">Regresar</p>\r\n          <button class=\"alta\" type=\"submit\">Guardar</button>\r\n        </div>\r\n      </form>\r\n\r\n      <div v-if=\"viewModal\" class=\"modal_view\">\r\n        <div id=\"modal_add\">\r\n          <div class=\"body dflx\">\r\n            <h3>Local agregado</h3>\r\n            <img src=\"../../assets/default/add.png\" alt=\"icono alerta\" />\r\n          </div>\r\n\r\n          <div class=\"modal_add_btns dflx\">\r\n            <p @click=\"Add()\" class=\"otro\">Aceptar</p>\r\n            <p @click=\"toEdit()\" class=\"editar\">Editar</p>\r\n            <p @click=\"back()\" class=\"inicio\">Inicio</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { mapActions } from \"vuex\";\r\nimport { VueEditor } from \"vue2-editor\";\r\nexport default {\r\n  name: \"AddLcl\",\r\n  components: {\r\n    VueEditor,\r\n  },\r\n  data() {\r\n    return {\r\n      status: \"\",\r\n      message: \"\",\r\n      id_colaborador: \"\",\r\n      colaborador: \"\",\r\n      titulo: \"\",\r\n      estatus: \"ACTIVO\",\r\n      renta: \"no\",\r\n      tipo: \"venta\",\r\n\r\n      descripcion: \"\",\r\n      ubicacion: \"\",\r\n      precio: 0,\r\n      superficie: \"\",\r\n      iframe: \"\",\r\n      detalles: \"\",\r\n      etiquetas: [],\r\n      etiqueta: \"\",\r\n      act_precio: \"ACTIVO\",\r\n      leyenda: \"\",\r\n      tipov: \"imagen\",\r\n      video_link: \"\",\r\n      //MODAL\r\n      files: [],\r\n      viewModal: false,\r\n\r\n      search_value: \"\",\r\n    };\r\n  },\r\n  async created() {\r\n    await this.getAllInfoClb(\"colaborador\");\r\n  },\r\n  computed: {\r\n    colaboradores() {\r\n      let data = this.$store.getters[\"colaborador/data\"];\r\n\r\n      if (!Array.isArray(data)) return [];\r\n      if (this.identity.role == \"colaborador\")\r\n        data = data.filter((item) => item.id == this.identity.sub);\r\n\r\n      if (this.search_value != \"\") {\r\n        let filt = data.filter((item) => {\r\n          const fullname = item.name + \" \" + item.lastname;\r\n          return this.includesItem(this.search_value, fullname);\r\n        });\r\n        data = filt;\r\n      }\r\n\r\n      return data;\r\n    },\r\n    identity() {\r\n      return this.$store.getters[\"admin/getIdentity\"];\r\n    },\r\n  },\r\n  methods: {\r\n    ...mapActions(\"locales\", [\"setAddedLcl\"]),\r\n    ...mapActions(\"locales\", [\"setHistoryOptionLcl\"]),\r\n    ...mapActions(\"colaborador\", [\"getAllInfoClb\"]),\r\n    ...mapActions(\"locales\", [\"getInfoByIdLcl\"]),\r\n    previewFiles() {\r\n      this.files = this.$refs.myFiles.files;\r\n    },\r\n    addEtiqueta(value) {\r\n      if (value) {\r\n        let newarr = [];\r\n        for (const item of this.etiquetas) {\r\n          newarr.push(item);\r\n        }\r\n        newarr.push(value);\r\n        this.etiquetas = newarr;\r\n        this.etiqueta = \"\";\r\n      }\r\n    },\r\n    delEtiqueta(index) {\r\n      let newarr = [];\r\n      for (var i = 0; i < this.etiquetas.length; i++) {\r\n        if (i != index) newarr.push(this.etiquetas[i]);\r\n      }\r\n      this.etiquetas = newarr;\r\n    },\r\n    wait: function () {\r\n      setTimeout(() => this.$router.go(), 200);\r\n    },\r\n    includesItem(search, name) {\r\n      name = name.normalize(\"NFD\").replace(/[\\u0300-\\u036f]/g, \"\");\r\n      if (search == \"\") {\r\n        return true;\r\n      }\r\n      name = name.toLowerCase();\r\n      search = search.toLowerCase();\r\n      if (search == \"\") {\r\n        return false;\r\n      }\r\n      return name.includes(search);\r\n    },\r\n    submit: async function (\r\n      id_colaborador,\r\n      titulo,\r\n      estatus,\r\n      renta,\r\n      tipo,\r\n      descripcion,\r\n      ubicacion,\r\n      precio,\r\n      superficie,\r\n      iframe,\r\n      detalles,\r\n      etiquetas,\r\n      act_precio,\r\n      leyenda,\r\n      files,\r\n      tipov,\r\n      video_link\r\n    ) {\r\n      this.status = \"\";\r\n      this.message = \"\";\r\n\r\n      if (tipov == \"imagen\") {\r\n        if (files.length > 0) {\r\n          let result = await this.$store.dispatch(\"locales/addItemLcl\", {\r\n            option: \"locales\",\r\n            item: {\r\n              id_colaborador,\r\n              titulo,\r\n              estatus,\r\n              renta,\r\n              tipo,\r\n              descripcion,\r\n              ubicacion,\r\n              precio,\r\n              superficie,\r\n              iframe,\r\n              detalles,\r\n              etiquetas,\r\n              act_precio,\r\n              leyenda,\r\n            },\r\n          });\r\n\r\n          if (result.status == \"error\") {\r\n            this.status = \"error\";\r\n            this.message = result.message;\r\n          } else {\r\n            // success\r\n            const id = result.added.id;\r\n\r\n            for (const image of files) {\r\n              //Creamos el formData\r\n              var data = new FormData();\r\n              //Añadimos la imagen seleccionada\r\n              data.append(\"image\", image);\r\n              data.append(\"video_link\", video_link);\r\n              data.append(\"tipov\", tipov);\r\n              data.append(\"id\", id);\r\n\r\n              //Añadimos el método PUT dentro del formData\r\n              // Como lo hacíamos desde un formulario simple _(no ajax)_\r\n              data.append(\"_method\", \"POST\");\r\n\r\n              await this.$store.dispatch(\"locales/addItemLcl\", {\r\n                option: \"locales/foto\",\r\n                item: data,\r\n              });\r\n            }\r\n\r\n            const newadded = await this.getInfoByIdLcl({\r\n              id: result.added.id,\r\n              option: \"locales\",\r\n            });\r\n            this.files = [];\r\n            this.setAddedLcl(newadded);\r\n            this.showModal();\r\n          }\r\n        } else {\r\n          this.status = \"error\";\r\n          this.message = \"No has adjuntado ninguna foto.\";\r\n          this.delStatus();\r\n        }\r\n      } else if (tipov == \"video\") {\r\n        // success\r\n        const id = result.added.id;\r\n        const image = \"\";\r\n        //Creamos el formData\r\n        /**/ var data = new FormData();\r\n        data.append(\"image\", \"\");\r\n        data.append(\"video_link\", video_link);\r\n        data.append(\"tipov\", tipov);\r\n        data.append(\"id\", id);\r\n        data.append(\"_method\", \"POST\");\r\n\r\n        await this.$store.dispatch(\"locales/addItemLcl\", {\r\n          option: \"locales/foto\",\r\n          item: data,\r\n        });\r\n\r\n        const newadded = await this.getInfoByIdLcl({\r\n          id: result.added.id,\r\n          option: \"locales\",\r\n        });\r\n        this.files = [];\r\n        this.setAddedLcl(newadded);\r\n        this.showModal();\r\n      }\r\n    },\r\n    delStatus: function () {\r\n      setTimeout(() => this.delMsgs(), 2000);\r\n    },\r\n    delMsgs: function () {\r\n      this.status = \"\";\r\n      this.message = \"\";\r\n    },\r\n\r\n    showModal: function () {\r\n      this.viewModal = true;\r\n    },\r\n\r\n    Add: function () {\r\n      this.status = \"\";\r\n      this.message = \"\";\r\n\r\n      this.id_colaborador = \"\";\r\n      this.titulo = \"\";\r\n      this.estatus = \"\";\r\n      this.renta = \"no\";\r\n      this.tipo = \"venta\";\r\n      this.descripcion = \"\";\r\n      this.ubicacion = \"\";\r\n      this.precio = \"\";\r\n      this.superficie = \"\";\r\n      this.iframe = \"\";\r\n      this.detalles = \"\";\r\n      this.etiquetas = [];\r\n\r\n      this.viewModal = false;\r\n      this.setAddedLcl(\"\");\r\n    },\r\n    toEdit: function () {\r\n      this.setHistoryOptionLcl(\"Edit\");\r\n      this.viewModal = false;\r\n    },\r\n    back: function () {\r\n      setTimeout(() => this.$router.go(), 200);\r\n    },\r\n\r\n    toolbar: function () {\r\n      return [\r\n        [\"bold\", \"italic\", \"underline\", \"strike\"], // toggled buttons\r\n        [\"blockquote\", \"code-block\"],\r\n\r\n        [{ header: 1 }, { header: 2 }], // custom button values\r\n        [{ list: \"ordered\" }, { list: \"bullet\" }],\r\n        [{ script: \"sub\" }, { script: \"super\" }], // superscript/subscript\r\n        [{ indent: \"-1\" }, { indent: \"+1\" }], // outdent/indent\r\n        [{ direction: \"rtl\" }], // text direction\r\n\r\n        [{ size: [\"small\", false, \"large\", \"huge\"] }], // custom dropdown\r\n        [{ header: [1, 2, 3, 4, 5, 6, false] }],\r\n        // [ 'link','image' ,'video' ],          // add's image support\r\n        [{ color: [] }, { background: [] }], // dropdown with defaults from theme\r\n        [{ font: [] }],\r\n        [{ align: [] }],\r\n\r\n        [\"clean\"], // remove formatting button\r\n      ];\r\n    },\r\n    toolbarvideo: function () {\r\n      return [[\"video\"]];\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n#AddLcl label {\r\n  color: var(--color-2);\r\n  font-weight: 700;\r\n  font-size: 0.7291666666666666vw;\r\n  margin-bottom: 0.3vw;\r\n}\r\n\r\n#AddLcl input,\r\n#AddLcl select {\r\n  height: 2.5vw;\r\n  width: 14.716666666666668vw;\r\n  border: 0.052083vw solid var(--color-2);\r\n  border-radius: 0vw 1.5vw 1.5vw 0vw;\r\n  color: var(--color-3);\r\n  font-size: 0.7291666666666666vw;\r\n  font-weight: 500;\r\n  padding-left: 1.09375vw;\r\n}\r\n\r\n#select-role {\r\n  height: 2.5vw;\r\n  width: 13.716667vw;\r\n  border: 0.052083vw solid var(--color-2);\r\n  border-radius: 0vw 1.5vw 1.5vw 0vw;\r\n  color: var(--color-3);\r\n  font-size: 0.7291666666666666vw;\r\n  font-weight: 500;\r\n  padding-left: 1.09375vw;\r\n  display: flex;\r\n  align-items: center;\r\n  background-image: url(\"../../assets/default/blueflecha.svg\");\r\n  background-repeat: no-repeat;\r\n  background-position-y: center;\r\n  background-position-x: 12.6vw;\r\n}\r\n#select-role.rounded {\r\n  border-radius: 0vw 1.5vw 0vw 0vw;\r\n  border-left: 0.052083vw solid var(--color-2);\r\n  border-right: 0.052083vw solid var(--color-2);\r\n  border-top: 0.052083vw solid var(--color-2);\r\n  border-bottom: 0;\r\n}\r\n#select-ch:checked ~ .options-cont {\r\n  display: flex;\r\n  position: absolute;\r\n  padding: 0.573vw 1.25vw;\r\n  flex-direction: column;\r\n  background-color: white;\r\n  width: 12.3vw;\r\n  border-left: 0.052083vw solid var(--color-2);\r\n  border-right: 0.052083vw solid var(--color-2);\r\n  border-top: 0;\r\n  border-bottom: 0.052083vw solid var(--color-2);\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}